#include<iostream>
#include<set>
using namespace std;
int main()
{
    string names[100] {"Bold", "Adya", "Ochir", "Naran", "Onon", "Anand", "Temujin", "Bud"};
    string input{};
    cout << "Enter name: ";
    cin >> input;
    cout << endl;
    for (int i = 0 ; i < 9 ; i++)
    {
        if(input == names[i]){
            cout << "Burtgeltei" << endl;
            cout << "At: " << i + 1 << endl;
            return 0;
        }
    }
    cout << "Burtgelgui" << endl;   
}
